{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "common-config": "^0.1.0",
    "create-react-class": "^15.6.2",
    "cross-env": "5.1.1",
    "element-react": "^1.2.1",
    "element-theme-default": "^1.4.6",
    "express": "^4.16.2",
    "immutable": "^3.8.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "prop-types": "^15.6.0",
    "rc-slider": "^8.3.1",
    "react": "16.0.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-alert": "^2.4.0",
    "react-bootstrap-daterangepicker": "^3.4.1",
    "react-dates": "^13.0.2",
    "react-dom": "^16.0.0",
    "react-form": "^2.10.0",
    "react-google-maps": "^9.2.2",
    "react-hot-loader": "3.1.1",
    "react-icons": "^2.2.5",
    "react-json-table": "^0.1.1",
    "react-moment": "^0.6.5",
    "react-places-autocomplete": "^5.4.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.15.4",
    "react-slider": "^0.9.0",
    "recharts": "^1.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2",
    "sha1": "^1.1.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.4",
    "html-webpack-plugin": "2.30.0",
    "husky": "0.15.0-beta.2",
    "jest": "21.2.0",
    "node-sass": "4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.1",
    "webpack": "3.8.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "2.9.4",
    "webpack-node-externals": "^1.6.0"
  }
}
